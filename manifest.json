{
    "manifest_version": 2,
    "type": "extension",
    "name": "背景插件",
    "slug": "background_plugin",
    "description": "在QQNT聊天界面轮播展示背景图，同时让QQNT界面透明化以便更好地展示背景图。",
    "version": "0.1.8",
    "thumbnail": "./icon.png",
    "author": [
        {
            "name": "XiaoHe321",
            "link": "https://github.com/xh321"
        }
    ],
    "repository": {
        "repo": "xh321/LiteLoaderQQNT-Background-Plugin",
        "branch": "master"
    },
    "platform": ["win32", "darwin", "linux"],
    "injects": {
        "renderer": "./renderer.js",
        "main": "./main.js",
        "preload": "./preload.js"
    }
}
